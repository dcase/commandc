<% content_for :sidebar do %>
	<%= render :partial => "common/portfoliolink" %>
	<% if @recent_projects %>
		<div id="recent-projects">
			<div class="large bold">recent projects:</div>
			<% for project in @recent_projects %>
				<% div_for project, :class => "recent-project" do %>
					<%= link_to image_tag(project.imagefile.public_filename(:project_thumb)), project_path(project) %>
					<div class="small">
						>> <%= link_to project.name, project_path(project) %>
					</div>
				<% end %>
			<% end %>
		</div>
	<% end %>
<% end %>
