<% # Begin Config section %>
<% content_for :title do %>COMMAND C > Contact Us<% end %>
<% content_for :bodyclass do %>contact<% end %>
<% # End Config section %>

<div id="inquiry_form">
 <%= render :partial => "contact_form", :locals => { :inquiry => @inquiry} %>
</div>

<% content_for :submenu do %>
	<div id="submenu">
		<%= image_tag 'inquire.gif', :class => 'submenu-heading', :size => '204x63', :alt => 'inquire' %>
		<div id="inquire_caption" class="small">please fill out this form to request a quote</div>
		<div id="contact_info">
			<p class="large bold">email us:<br /><span class="red">info@commandc.com</span></p>
			<p class="large bold">call us:<br /><span class="red">347.844.9208</span></p>
		</div>
	</div><!-- EOF DIV 'submenu' -->
<% end %>

<% content_for :sidebar do %>
	<%= render :partial => "common/portfoliolink" %>
<% end %>
