<h1>Listing inquiries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Timeframe</th>
    <th>Email</th>
    <th>Budget</th>
    <th>Phone</th>
    <th>Reachable at</th>
    <th>Url</th>
    <th>Source</th>
    <th>Description</th>
  </tr>

<% for inquiry in @inquiries %>
  <tr>
    <td><%=h inquiry.name %></td>
    <td><%=h inquiry.timeframe %></td>
    <td><%=h inquiry.email %></td>
    <td><%=h inquiry.budget %></td>
    <td><%=h inquiry.phone %></td>
    <td><%=h inquiry.reachable_at %></td>
    <td><%=h inquiry.url %></td>
    <td><%=h inquiry.source %></td>
    <td><%=h inquiry.description %></td>
    <td><%= link_to 'Show', inquiry %></td>
    <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
    <td><%= link_to 'Destroy', inquiry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New inquiry', new_inquiry_path %>
